package com;

import java.util.Scanner;

public class BuzzNumber {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		System.out.println("Enter a number");
		int n = sc.nextInt();
		boolean rs = isBuzz(n);


	}

	static boolean isBuzz(int n) {
		if(n/7==0||n%7==0) {
			return true;
		}

			return false;
	}

}


